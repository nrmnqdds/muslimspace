{
  "$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
  "organizeImports": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "suspicious": {
        "noArrayIndexKey": "off"
      }
    },
    "ignore": [
      "node_modules",
      "pnpm-lock.yaml",
      "src/routeTree.gen.ts"
    ]
  },
  "formatter": {
    "enabled": true,
    "indentStyle": "space",
    "indentWidth": 2,
    "ignore": [
      "node_modules",
      "pnpm-lock.yaml",
      "src/routeTree.gen.ts"
    ]
  }
}
